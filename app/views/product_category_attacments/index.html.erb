<p id="notice"><%= notice %></p>

<h1>Listing Product Category Attacments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_category_attacments.each do |product_category_attacment| %>
      <tr>
        <td><%= link_to 'Show', product_category_attacment %></td>
        <td><%= link_to 'Edit', edit_product_category_attacment_path(product_category_attacment) %></td>
        <td><%= link_to 'Destroy', product_category_attacment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

